grammar mm.ecxt.MMLanguage with org.eclipse.xtext.common.Terminals

generate mmLanguage "urn:marnix:mm.exct/MMLanguage"

MetamathDatabase:
	statements+=statement*;
	
statement:
	constantStatement;
	
constantStatement:
    '$c' id=MATHSYMBOL '$.';
    
terminal MATHSYMBOL: 
      ('!'..'#'|'%'..'~')+; 
